    "#### 12. Write a program to find out factorial using recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "d7ad988f-61df-4111-9c68-6c3dbc962808",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter Number 3\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Factorial of Number is : 6\n"
     ]
    }
   ],
   "source": [
    "def factorial(no):\n",
    "    if(no == 1):\n",
    "        fact = 1\n",
    "    else:\n",
    "        fact = no*factorial(no-1)\n",
    "    return fact\n",
    "    \n",
    "no = int(input(\"Enter Number\"))\n",
    "print(\"Factorial of Number is :\", factorial(no))"
   ]
  }
 ],
